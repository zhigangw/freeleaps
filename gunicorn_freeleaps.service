[Unit]
Description=Lingyuan Yan gunicorn daemon
Requires=gunicorn_freeleaps.socket
After=network.target

[Service]
User=wwwadmin
Group=wwwadmin
WorkingDirectory=/home/wwwadmin/freeleaps
ExecStart=/home/wwwadmin/freeleaps/venv_t/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/wwwadmin/freeleaps/freeleaps.sock freeleaps.wsgi:application

[Install]
WantedBy=multi-user.target

